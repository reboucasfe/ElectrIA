// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Placeholder model to allow `prisma generate` to succeed.
// You can replace this with your actual data models later.
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}